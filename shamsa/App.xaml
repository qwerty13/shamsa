<Application x:Class="shamsa.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:shamsa"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="Dio_font">
            <Setter Property="TextElement.FontFamily" Value="fonts/#DiodrumArabic ☞"/>
        </Style>
        <Style x:Key="Segoe_font">
            <Setter Property="TextElement.FontFamily" Value="fonts/#Segoe UI"/>
        </Style>
    </Application.Resources>
</Application>
